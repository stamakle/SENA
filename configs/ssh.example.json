{
  "default_user": "user",
  "default_port": 22,
  "default_key_path": "~/.ssh/id_ed25519",
  "default_password": "",
  "allowlist": [
    "uname -a",
    "sudo",
    "sudo -n true",
    "uname -n",
    "hostname",
    "lscpu",
    "lsblk -o NAME,SIZE,MODEL,SERIAL",
    "lspci -nn",
    "lspci",
    "lspci -vv",
    "lspci -vv | head -n 200",
    "cat /etc/os-release",
    "ip -4 addr show",
    "nvme list",
    "nvme fw-log /dev/nvme0",
    "sudo nvme list",
    "nvme fw-log /dev/nvme0",
    "nvme smart-log /dev/nvme0",
    "nvme error-log /dev/nvme0",
    "dmesg | tail -n 50",
    "dmesg | tail -n 200",
    "dmesg -T --level=err,crit,alert,emerg | tail -n 200",
    "journalctl -k -p 3 -b --no-pager | tail -n 200",
    "sudo -n true",
    "nvme list and fetch nvme id-ctrl",
    "nvme id-ctrl",
    "nvme id-ctrl  /dev/nvme0n1",
    "nvme id-ctrl /dev/nvme0n1",
    "python3 /tmp/sena_tools/TC-3362/TC-3362_physical_slot_num.py --log-folder /tmp/sena_tools/TC-3362",
    "nvme smart-log /dev/nvme1",
    "nvme error-log /dev/nvme1",
    "nvme fw-log /dev/nvme1",
    "python3 /tmp/sena_tools/DSSTC-5351/dsstc-5351_fio.py --device /dev/nvme0n1 --execute"
  ],
  "timeout_sec": 30
}
